{
  "compilerOptions": {
    "target": "ES2022",
    "jsx": "react-jsx",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "ES2022",
    "moduleResolution": "node10",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "allowImportingTsExtensions": true,
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
